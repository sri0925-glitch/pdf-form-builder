{
  "name": "pdf-form-builder",
  "version": "1.1.0",
  "description": "Programmatically adds fillable AcroForm fields to a PDF using JSON coordinate mapping, with optional LLM content generation",
  "type": "module",
  "bin": {
    "pdf-form-builder": "./add-fields.mjs"
  },
  "scripts": {
    "inspect": "node add-fields.mjs inspect",
    "build": "node add-fields.mjs build",
    "extract": "node extract-previous.mjs",
    "generate": "node generate-content.mjs",
    "ees": "node ees-filler.mjs",
    "measure": "node measure-coords.mjs",
    "export-word": "node export-word.mjs"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "docx": "^9.5.1",
    "pdf-lib": "^1.17.1",
    "playwright": "^1.58.1"
  }
}
